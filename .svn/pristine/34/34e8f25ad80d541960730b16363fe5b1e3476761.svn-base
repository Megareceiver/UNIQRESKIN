<SupplyLines>
	<CustomerName><![CDATA[{$inv->debtor_name}]]></CustomerName>
	<CustomerUEN>{$inv->gst_no}</CustomerUEN>
	<InvoiceDate>{date_format time=$inv->tran_date}</InvoiceDate>
	<InvoiceNo>{$inv->reference}</InvoiceNo>
	<LineNo>{$line_no}</LineNo>
	<ProductDescription><![CDATA[{if $inv->type==$smarty.const.ST_SALESINVOICE}{$inv->item_memo}{else}{$inv->long_description}{/if}]]></ProductDescription>
	<SupplyValueSGD>{number_format num=$price }</SupplyValueSGD>
	<GSTValueSGD>{number_format num=$tax_value }</GSTValueSGD>
	<TaxCode>{$tax_code}</TaxCode>
	<Country>{$inv->charge_to_address}</Country>
	<FCYCode>{if $inv->currency_code != $curr_default}{$inv->currency_code}{/if}</FCYCode>
	<SupplyFCY>{number_format num=$price_curr }</SupplyFCY>
	<GSTFCY>{number_format num=$tax_curr }</GSTFCY>
</SupplyLines>