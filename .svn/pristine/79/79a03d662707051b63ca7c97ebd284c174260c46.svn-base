    left: 0;
    z-index: 1050;
    display: none;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
}
.fade.in {
    opacity: 1;
	display: block;
}

.modal-backdrop {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1001;
    background-color: #000;
}
.modal-backdrop.in {
    filter: alpha(opacity=5);
    opacity: 0.05;
}

.modal-processing span.text {
	position: relative;
	top: 117px;
}

.modal-processing {
    width: 150px;
	height: 50px;
    margin: 50px auto;

	position: relative;
	 z-index: 1002;
	text-align: center;
	font-weight: bold;
	color: #3eb34e;
	font-size: 15px;
}
.modal-processing span {
	display: block;
}


