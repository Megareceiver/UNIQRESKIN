#nav li#myFavouritesMenuDropDown:hover a.dropdown {
    background-color: transparent;
}
#nav #myFavouritesMenuDropDown a.dropdown {
    height: 16px;
}
.favouritesMenu-image {
    background: url("/resources/images_0004/favouriteSageOrange.png") no-repeat scroll 0 0 transparent;
    height: 16px;
    margin-top: 7px;
    width: 16px;
}
.favouritesMenu-image:hover {
    background-position: 0 -16px;
}
.helpMenuDropdown dd, .helpMenuDropdown dt, .helpMenuDropdown ul {
    margin: 0;
    padding: 0;
    position: relative;
}
.helpMenuDropdown dd {
    position: relative;
}
#nav-stub .helpMenuDropdown a, .helpMenuDropdown a:visited {
    color: #666;
    outline: medium none;
    text-decoration: none;
}
#nav-stub .helpMenuDropdown a.last:hover {
    color: #fff;
    text-decoration: underline;
}
#nav-stub .helpMenuDropdown a:hover {
    color: #36b34a;
    text-decoration: underline;
}
.helpMenuDropdown dt a:hover {
    color: #36b34a;
    text-decoration: underline;
}
.helpMenuDropdown dt a {
    background: url("/resources/images_0004/helpMenuDropdownArrow.png") no-repeat scroll right center rgba(0, 0, 0, 0);
    color: #666;
    padding-right: 6px;
    text-decoration: none;
}
.helpMenuDropdown dt a span {
    cursor: pointer;
    display: inline-block;
    padding-left: 0;
    padding-right: 5px;
}
.helpMenuDropdown dd ul {
    background: none repeat scroll 0 0 #fff;
    border: 1px solid #666;
    clear: right;
    color: #333;
    display: none;
    float: right;
    left: 0;
    list-style: outside none none;
    min-width: 140px;
    padding: 5px 0;
    position: absolute;
    top: 2px;
    width: auto;
    z-index: 1000;
}
.helpMenuDropdown span.value {
    display: none;
}
.helpMenuDropdown dd ul li a {
    display: block;
    padding: 5px;
}
.divHelpMenuDropdown-onTop {
    z-index: 9999;
}
.additional-padding {
    padding-left: 3px;
    padding-right: 3px;
}
#nav .prelogin li, #nav .prelogin ul, #nav .prelogin a, #nav .prelogin .sub-level li a span {
    cursor: pointer;
}
#nav {
    position: relative;
    z-index: 1000;
}
#nav > ul {
    background-color: #ededed;
    height: 42px;
    list-style-type: none;
    margin-bottom: 0;
    margin-top: 0;
    padding: 0;
}
#nav li {
    cursor: default;
    float: left;
    position: relative;
	z-index:8000;
}
#nav >ul >li:hover:after,#nav >ul >li.active:after{
 border-left: 8px solid transparent;
    border-right: 8px solid transparent;
    border-top: 8px solid #2eb544;
    content: "";
    left: 50%;
    margin-left: -8px;
    position: absolute;
    top: 100%;
    z-index: 8000;
}
#nav >ul >li:hover,#nav >ul >li.active{
	color:#FFF;
	background:#2eb544;
}
#nav >ul >li >a:hover,#nav >ul >li.active >a{
    color:#FFF;
    background:#2eb544;
}
#nav a img {
	float: left;
    margin-right: 5px;
    margin-top: 15px;
}
#nav a span{   line-height: 43px;}
#nav a {
    background-position: 0 -31px;
    border-left: 1px solid transparent;
    border-right: 1px solid transparent;
    color: #131313;
    text-decoration: none;
	 display: block;
    font-size: 12px;
    height: 42px;
    padding: 0 15px;
}
#nav .topmenubarContainer {
    margin-left: auto;
    margin-right: auto;
    position: relative;
    width: 1010px;
}
.layout-body-loggedIn #nav li.first a.dropdownWithHref span {
}
.layout-body-loggedOut #nav li:hover a.selected {
    background-color: #ffffff;
}
.layout-body-loggedOut #nav a span {
    display: block;
    font-size: 12px;
    height: 31px;
    line-height: 32px;
    padding: 0 8px;
}

#nav a.selected, #nav a.selected:hover {
    background-color: #fff;
    background-position: 0 -93px;
    border-left: 1px solid #ededed;
    border-right: 1px solid #ededed;
    color: #222;
    font-weight: bold;
}
#nav .first a {
    background-position: 0 0;
}
#nav .first a:hover {
       background: #2eb544;
}
#nav .first a.selected, #nav .first a.selected:hover {
    background-position: 0 -62px;
}
#nav a.dropdown span {
    cursor: default;
    padding-right: 20px;
}
#nav a.dropdown.selected span, #nav a.dropdown.selected:hover span {
    background-position: right -293px;
}
#nav a.dropdownWithHref span {
    cursor: pointer;
  /*  padding-right: 20px;*/
}
#nav a.dropdownWithHref.selected span, #nav a.dropdownWithHref.selected:hover span {
    background-position: right -293px;
}
#nav .sub-level {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: url("../images/menumt2.png") no-repeat;
    border-color: -moz-use-text-color;
    border-image: none;
    border-right: medium none;
    border-style: none;
    border-width: 0 medium medium;
    display: none;
    margin: 0;
    position: absolute;
    top: 42px;
    z-index: 7000;
}
#nav .sub-level.visible {
    display: block;
}
#nav .sub-level li {
    float: none;
    margin: 0;

}
#nav ul.sub-level a, #nav ul.sub-level1 a, #nav ul.sub-level2 a, #nav ul.sub-level li a, #nav ul.sub-level1 li a, #nav ul.sub-level2 li a {
    background-color: #cecece;
    border: medium none;
    padding-bottom: 2px;
    padding-top: 2px;
}
#nav ul.sub-level a:hover, #nav ul.sub-level1 a:hover, #nav ul.sub-level2 a:hover, #nav ul.sub-level li a:hover, #nav ul.sub-level1 li a:hover, #nav ul.sub-level2 li a:hover {
    background-color: #bfbfbf;
}
#nav .sub-level li a {
    background: none repeat scroll 0 0 #fff;
    height: 26px;
    padding-right: 20px;
    white-space: nowrap;
}
#nav .sub-level .seperatorTop > li > a {
    border-top: 1px solid #e3e3e3;
    outline: 1px solid #fff;
}
#nav .sub-level .seperatorBottom > li > a {
    border-bottom: 1px solid #e3e3e3;
    outline: 1px solid #e3e3e3;
}
#nav .sub-level li a:hover {
    background-color: #fcfcfc;
    color: #222;
}
#nav .sub-level li.first a {
    border-top: 0 none;
}
#nav .sub-level li.last > a {
    border-bottom: 0 none;
}
#nav .sub-level li.seperatorBottom > a {
}
#nav .sub-level li a span {
    height: 26px;
    line-height: 26px;
    padding-left: 10px;
}
#nav a.dropside span {
    cursor: default;
    padding-right: 0;
}
#nav .sub-level1 {
    background: none repeat scroll 0 0 #fff;
    display: none;
    position: absolute;
    top: 0;
    z-index: 7000;
}
#nav .sub-level1.visible {
    display: block;
}
#nav .sub-level1 li {
    float: none;
    margin: 0;
    min-width: 170px;
}
#nav .sub-level1 li a {
    background: none repeat scroll 0 0 #fff;
    height: 26px;
    padding-right: 5px;
