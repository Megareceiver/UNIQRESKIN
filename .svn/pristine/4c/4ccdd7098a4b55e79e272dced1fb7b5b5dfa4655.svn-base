}
#nav .sub-level1 li a:hover {
    background-color: #fcfcfc;
    color: #222;
}
#nav .sub-level1 li.first a {
    border-top: 0 none;
}
#nav .sub-level1 li.last a {
    border-bottom: 0 none;
}
