/* button */
.ajaxsubmit, .navibutton, .inputsubmit {
   background:#2eb544;color:#FFF;font-size:16px;padding:5px 30px;line-height:0;
}
.ajaxsubmit, .navibutton, .inputsubmit {
    -moz-user-select: none;
    background-image: none;
    border: 1px solid transparent;
    cursor: pointer;
    display: inline-block;
    font-size: 12px;
    font-weight: 400;
    line-height: 1.42857;
    margin-bottom: 0;
    padding: 6px 12px;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;
    text-decoration: none;
	margin: auto 3px;
}
